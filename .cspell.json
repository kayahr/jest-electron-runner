{
    "version": "0.2",
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "enabledLanguageIds": [
        "markdown",
        "typescript"
    ],
    "files": [
        "**/*"
    ],
    "ignorePaths": [
        "node_modules",
        "coverage",
        "package-lock.json",
        "git",
        "lib",
        "*.log",
        "*.tgz"
    ],
    "words": [
        "copyfiles",
        "docblock",
        "docblocks",
        "IPCID",
        "kayahr",
        "Reimer",
        "respawn",
        "webgl",
        "webgpu"
    ],
    "language": "en",
    "enableFiletypes": [
        "json"
    ]
}
